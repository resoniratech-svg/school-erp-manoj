{
  "name": "@school-erp/shared",
  "version": "1.0.0",
  "description": "Shared utilities, types, and constants for School ERP",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./errors": {
      "import": "./dist/errors/index.mjs",
      "require": "./dist/errors/index.js",
      "types": "./dist/errors/index.d.ts"
    },
    "./permissions": {
      "import": "./dist/permissions/index.mjs",
      "require": "./dist/permissions/index.js",
      "types": "./dist/permissions/index.d.ts"
    },
    "./pagination": {
      "import": "./dist/pagination/index.mjs",
      "require": "./dist/pagination/index.js",
      "types": "./dist/pagination/index.d.ts"
    },
    "./api": {
      "import": "./dist/api/index.mjs",
      "require": "./dist/api/index.js",
      "types": "./dist/api/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./constants": {
      "import": "./dist/constants/index.mjs",
      "require": "./dist/constants/index.js",
      "types": "./dist/constants/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "license": "UNLICENSED",
  "private": true
}
