{
    "name": "@school-erp/api-client",
    "version": "1.0.0",
    "description": "Typed Web API Client SDK for School ERP",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "lint": "eslint src --ext .ts",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "axios": "^1.6.0",
        "zod": "^3.22.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "tsup": "^8.0.0",
        "typescript": "^5.3.0"
    },
    "peerDependencies": {},
    "files": [
        "dist"
    ],
    "license": "MIT"
}